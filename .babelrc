{
	"presets": [
			"@babel/typescript",
			["@babel/preset-env", { "debug": true, "useBuiltIns": "entry", "corejs": 3 }]
	],
	"plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    ["@babel/transform-runtime", { "corejs": 3 }]    
	]
}
